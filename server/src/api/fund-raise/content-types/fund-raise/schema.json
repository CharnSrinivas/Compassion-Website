{
  "kind": "collectionType",
  "collectionName": "fund_raises",
  "info": {
    "singularName": "fund-raise",
    "pluralName": "fund-raises",
    "displayName": "fund-raise",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fund_target": {
      "type": "decimal",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text",
      "required": false
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "tag": {
      "type": "enumeration",
      "enum": [
        "medical",
        "memorial",
        "emergency",
        "education",
        "funeral",
        "animals",
        "environment",
        "business",
        "community",
        "competition",
        "creative",
        "event",
        "faith",
        "family",
        "sports",
        "travel",
        "volunteer",
        "wishes",
        "other"
      ],
      "required": true
    },
    "fund_raised": {
      "type": "decimal",
      "required": false,
      "default": 0
    },
    "slug": {
      "type": "uid",
      "required": true,
      "minLength": 5,
      "maxLength": 120,
      "targetField": "title"
    },
    "donations_count": {
      "type": "biginteger",
      "default": "0",
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "fund_raises"
    },
    "donations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::donation.donation",
      "mappedBy": "fund_raise"
    },
    "charity": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::charity.charity",
      "inversedBy": "fundraisers"
    },
    "individual": {
      "type": "boolean",
      "default": true
    },
    "fund_type": {
      "type": "enumeration",
      "enum": [
        "INR",
        "USD",
        "EURO",
        "BITCOIN"
      ]
    },
    "address": {
      "type": "string",
      "required": true
    },
    "recv_details": {
      "type": "string",
      "required": true
    }
  }
}
